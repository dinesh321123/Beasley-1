{
	"name": "beasley/wp-content",
	"repositories": [
		{
			"type": "composer",
			"url": "https://wpackagist.org"
		}
	],
	"require": {
		"humanmade/s3-uploads": "~2.0",
		"oomphinc/composer-installers-extender": "^1.1",
		"wpackagist-plugin/advanced-custom-fields": "^5.8.6",
		"wpackagist-plugin/fb-instant-articles": "^4.2.0",
		"wpackagist-plugin/duplicate-post": "^3.2.3",
		"wpackagist-plugin/edit-flow": "^0.9.1",
		"wpackagist-plugin/elasticpress": "^3.2.2",
		"wpackagist-plugin/feed-pull": "^0.2.6",
		"wpackagist-plugin/public-post-preview": "^2.9.0",
		"wpackagist-plugin/restricted-site-access": "^7.1.0",
		"wpackagist-plugin/term-management-tools": "^1.1.4",
		"wpackagist-plugin/the-events-calendar": "^4.9.10",
		"wpackagist-plugin/wordpress-importer": "^0.6.4",
		"wpackagist-plugin/wp-crontrol": "^1.7.1",
		"wpackagist-plugin/wordpress-seo": "^12.4",
		"wpackagist-plugin/safe-redirect-manager": "^1.9.2",
		"wpackagist-plugin/simple-local-avatars": "^2.1.1",
		"wpackagist-plugin/wp-fail2ban": "^4.2.7.1",
		"wpackagist-plugin/disable-real-mime-check": "^1.0",
		"wpackagist-plugin/query-monitor": "^3.4.0",
		"wpackagist-plugin/rewrite-rules-inspector": "^1.2.1",
		"wpackagist-plugin/debug-bar": "*",
    "wpackagist-plugin/debug-bar-elasticpress": "*",
    "wpackagist-plugin/wp-newrelic":"^1.3",
    "wpackagist-plugin/classic-editor":"^1.5",
		"10up/wp-minions": "^4.1.0",
		"10up/post-finder": "^0.3.0"
	},
	"require-dev": {
		"wpackagist-plugin/user-switching": "^1.5.3"
	},
	"autoload": {
		"psr-0": {
			"Bbgi": "mu-plugins/classes"
		},
		"files": [
			"mu-plugins/functions/term-data-store.php",
			"mu-plugins/functions/imageflow.php",
			"mu-plugins/functions/post.php",
			"mu-plugins/functions/settings.php"
		]
	},
	"extra": {
		"installer-paths": {
			"mu-plugins/{$name}": [
				"wpackagist-plugin/advanced-custom-fields",
				"10up/post-finder"
			],
			"plugins/{$name}/": [
				"type:wordpress-plugin"
			]
		}
	}
}

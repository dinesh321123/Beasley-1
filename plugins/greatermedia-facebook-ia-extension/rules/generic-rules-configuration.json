{
  "rules":
  [
    {
      "class": "InteractiveRule",
      "selector": "blockquote.embedly-card",
      "properties": {
        "interactive.iframe": {
          "type": "multiple",
          "children": [
            {
              "type": "element",
              "selector": "blockquote"
            },
            {
              "type": "next-sibling-element-of",
              "selector": "blockquote"
            }
          ]
        }
      }
    }, {
      "class": "PassThroughRule",
      "selector": "blockquote h4"
    }, {
      "class": "PassThroughRule",
      "selector" : "hr"
    }
  ]
}